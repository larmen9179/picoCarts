pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
function _init()
	ball={}
	ball.x=0
	ball.y=0
	ball.r=2
	routines={} --storing routines
	t=30
end

function _update()
	if t==30 do
		t=1
		pos=rnd(128)
		ball.x,ball.y=pos,pos
	end
	t+=1
end

function _draw()
	--cls()
	circfill(ball.x, ball.y, ball.r, 5)
end

function async()

end

